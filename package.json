{
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest --maxWorkers=4 src --coverage",
    "lint": "yarn eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json,css,md}'",
    "typecheck": "tsc --noEmit"
